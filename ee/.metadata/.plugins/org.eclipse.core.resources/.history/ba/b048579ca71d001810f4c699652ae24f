package Recursion_Lab;
import java.util.LinkedList;
import java.util.ListIterator;

	public class MyLinkedList extends  LinkedList<String>   {
		 LinkedList<String> inter;
		 public MyLinkedList() {
			 inter= super().listIterator();
			
			
			}
		
		
		String findLargest(String larg) {
			String large;
			System.out.println(larg);
			if(!((ListIterator<String>) inter).hasNext()) {
				large = larg;
			}else {String temp = ((ListIterator<String>) inter).next();
			if(larg.compareTo(temp)>0) {
			large = findLargest(larg);
		}else {
			large = findLargest(temp);	
		}
	}
			return large;
		}


		public String findLargest() {
		String Largest;
		Largest= ((ListIterator<String>) inter).next();
		if(((ListIterator<String>) inter).hasNext()) {
			Largest = findLargest(Largest);
		}
		return Largest;
		
		
		
		}
		

		}


	